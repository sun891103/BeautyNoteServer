<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
<mapper namespace="sample">
     <select id="selectBoardList" resultType="beautynote.common.vo.CM_StoreVO">
        SELECT STOR_CD AS storCd
        	 , STOR_NM AS storNm
          FROM CM_STORE
          WHERE STOR_CD = 820
    </select>
</mapper>
